{
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "endOfLine": "auto",
  "bracketSameLine": true,
  "jsxSingleQuote": true,
  "parser": "typescript",
  "printWidth": 120,
  "proseWrap": "never",
  "semi": true,
  "singleAttributePerLine": true,
